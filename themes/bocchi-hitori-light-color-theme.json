{
    "name": "Bocchi - Hitori Light",
    "type": "light",
    "colors": {
        // 编辑器核心
        "editor.background": "#F6E9E6",
        "editor.foreground": "#010001",
        "editor.lineHighlightBackground": "#F4A5B020",
        "editor.selectionBackground": "#F4A5B040",
        "editor.selectionHighlightBackground": "#F4A5B030",
        "editor.findMatchBackground": "#F4A5B050",
        "editor.findMatchHighlightBackground": "#F4A5B030",
        
        // 编辑器装饰
        "editorLineNumber.foreground": "#B4698C",
        "editorLineNumber.activeForeground": "#010001",
        "editorIndentGuide.background": "#B4698C40",
        "editorIndentGuide.activeBackground": "#B4698C80",
        "editorBracketMatch.background": "#F4A5B030",
        "editorBracketMatch.border": "#5C015C",
        
        // 侧边栏
        "sideBar.background": "#FDF6F7",
        "sideBar.foreground": "#010001",
        "sideBarTitle.foreground": "#010001",
        "sideBarSectionHeader.background": "#F4A5B030",
        "sideBarSectionHeader.foreground": "#010001",
        
        // 活动栏
        "activityBar.background": "#F4A5B0",
        "activityBar.foreground": "#010001",
        "activityBar.inactiveForeground": "#B4698C",
        "activityBarBadge.background": "#5C015C",
        "activityBarBadge.foreground": "#F6E9E6",
        
        // 标题栏
        "titleBar.activeBackground": "#F4A5B0",
        "titleBar.activeForeground": "#010001",
        "titleBar.inactiveBackground": "#F4A5B0CC",
        "titleBar.inactiveForeground": "#B4698C",
        
        // 状态栏
        "statusBar.background": "#F4A5B0",
        "statusBar.foreground": "#010001",
        "statusBar.debuggingBackground": "#5C015C",
        "statusBar.noFolderBackground": "#F4A5B0",
        
        // 标签页
        "tab.activeBackground": "#F6E9E6",
        "tab.inactiveBackground": "#FDF6F7",
        "tab.activeForeground": "#010001",
        "tab.inactiveForeground": "#B4698C",
        "tab.activeBorderTop": "#5C015C",
        "tab.hoverBackground": "#F4A5B020",
        
        // 列表和树
        "list.activeSelectionBackground": "#F4A5B0",
        "list.activeSelectionForeground": "#010001",
        "list.inactiveSelectionBackground": "#F4A5B050",
        "list.hoverBackground": "#F4A5B020",
        "list.highlightForeground": "#5C015C"
    },
    "tokenColors": [
        {
            "name": "Keywords",
            "scope": [
                "keyword",
                "storage.type",
                "storage.modifier",
                "keyword.control",
                "keyword.operator.new"
            ],
            "settings": {
                "foreground": "#5C015C",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Types",
            "scope": [
                "entity.name.type",
                "entity.name.class",
                "entity.name.struct",
                "entity.name.enum",
                "entity.name.interface",
                "support.type",
                "support.class"
            ],
            "settings": {
                "foreground": "#B4698C",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Functions",
            "scope": [
                "entity.name.function",
                "support.function",
                "meta.function-call"
            ],
            "settings": {
                "foreground": "#5C015C"
            }
        },
        {
            "name": "Variables",
            "scope": [
                "variable",
                "variable.other",
                "variable.parameter",
                "variable.language"
            ],
            "settings": {
                "foreground": "#010001"
            }
        },
        {
            "name": "Constants",
            "scope": [
                "constant",
                "constant.language",
                "constant.character",
                "constant.other"
            ],
            "settings": {
                "foreground": "#5C015C",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Numbers",
            "scope": [
                "constant.numeric",
                "constant.numeric.integer",
                "constant.numeric.float",
                "constant.numeric.hex",
                "constant.numeric.octal",
                "constant.numeric.binary",
                "constant.numeric.decimal"
            ],
            "settings": {
                "foreground": "#B4698C"
            }
        },
        {
            "name": "Strings",
            "scope": [
                "string",
                "string.template",
                "string.quoted"
            ],
            "settings": {
                "foreground": "#B4698C"
            }
        },
        {
            "name": "Comments",
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "foreground": "#A8969299",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Operators",
            "scope": [
                "keyword.operator",
                "punctuation.definition"
            ],
            "settings": {
                "foreground": "#5C015C"
            }
        },
        {
            "name": "Special Operators",
            "scope": [
                "keyword.operator.arithmetic",
                "keyword.operator.comparison",
                "keyword.operator.logical",
                "keyword.operator.assignment"
            ],
            "settings": {
                "foreground": "#B4698C"
            }
        },
        {
            "name": "Punctuation",
            "scope": [
                "punctuation",
                "punctuation.definition.block",
                "punctuation.definition.parameters",
                "punctuation.section",
                "meta.brace"
            ],
            "settings": {
                "foreground": "#B4698C"
            }
        },
        {
            "name": "Object Keys",
            "scope": [
                "meta.object-literal.key",
                "variable.object.property",
                "support.type.property-name"
            ],
            "settings": {
                "foreground": "#5C015C"
            }
        },
        {
            "name": "Language Constants",
            "scope": [
                "constant.language",
                "constant.language.boolean",
                "constant.language.null",
                "constant.language.undefined",
                "variable.language.this",
                "variable.language.super"
            ],
            "settings": {
                "foreground": "#B4698C",
                "fontStyle": "italic bold"
            }
        },
        {
            "name": "Function Parameters",
            "scope": [
                "variable.parameter"
            ],
            "settings": {
                "foreground": "#010001",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Import/Export Keywords",
            "scope": [
                "keyword.control.import",
                "keyword.control.export",
                "keyword.control.from"
            ],
            "settings": {
                "foreground": "#5C015C",
                "fontStyle": "bold"
            }
        },
        {
            "name": "HTML/XML Tags",
            "scope": [
                "entity.name.tag",
                "punctuation.definition.tag"
            ],
            "settings": {
                "foreground": "#5C015C"
            }
        },
        {
            "name": "HTML/XML Attributes",
            "scope": [
                "entity.other.attribute-name"
            ],
            "settings": {
                "foreground": "#B4698C",
                "fontStyle": "italic"
            }
        },
        {
            "name": "CSS Properties",
            "scope": [
                "support.type.property-name",
                "support.type.vendored.property-name"
            ],
            "settings": {
                "foreground": "#5C015C"
            }
        },
        {
            "name": "CSS Values",
            "scope": [
                "support.constant.property-value",
                "support.constant.vendored.property-value"
            ],
            "settings": {
                "foreground": "#B4698C"
            }
        },
        {
            "name": "Markdown Headers",
            "scope": [
                "markup.heading"
            ],
            "settings": {
                "foreground": "#5C015C",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown Links",
            "scope": [
                "markup.underline.link",
                "string.other.link"
            ],
            "settings": {
                "foreground": "#B4698C"
            }
        }
    ]
} 