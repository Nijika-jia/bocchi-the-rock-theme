{
    "name": "Bocchi - Ryo Light",
    "type": "light",
    "colors": {
        // 编辑器核心
        "editor.background": "#E6DBD6",
        "editor.foreground": "#000202",
        "editor.lineHighlightBackground": "#375B9D15",
        "editor.selectionBackground": "#375B9D30",
        "editor.selectionHighlightBackground": "#375B9D20",
        "editor.findMatchBackground": "#375B9D40",
        "editor.findMatchHighlightBackground": "#375B9D20",
        
        // 编辑器装饰
        "editorLineNumber.foreground": "#353642",
        "editorLineNumber.activeForeground": "#000202",
        "editorIndentGuide.background": "#35364230",
        "editorIndentGuide.activeBackground": "#35364260",
        "editorBracketMatch.background": "#375B9D30",
        "editorBracketMatch.border": "#035555",
        
        // 侧边栏
        "sideBar.background": "#F0E8E4",
        "sideBar.foreground": "#000202",
        "sideBarTitle.foreground": "#000202",
        "sideBarSectionHeader.background": "#375B9D20",
        "sideBarSectionHeader.foreground": "#000202",
        
        // 活动栏
        "activityBar.background": "#375B9D",
        "activityBar.foreground": "#E6DBD6",
        "activityBar.inactiveForeground": "#E6DBD680",
        "activityBarBadge.background": "#035555",
        "activityBarBadge.foreground": "#E6DBD6",
        
        // 标题栏
        "titleBar.activeBackground": "#375B9D",
        "titleBar.activeForeground": "#E6DBD6",
        "titleBar.inactiveBackground": "#375B9DCC",
        "titleBar.inactiveForeground": "#E6DBD680",
        
        // 状态栏
        "statusBar.background": "#375B9D",
        "statusBar.foreground": "#E6DBD6",
        "statusBar.debuggingBackground": "#035555",
        "statusBar.noFolderBackground": "#375B9D",
        
        // 标签页
        "tab.activeBackground": "#E6DBD6",
        "tab.inactiveBackground": "#F0E8E4",
        "tab.activeForeground": "#000202",
        "tab.inactiveForeground": "#35364299",
        "tab.activeBorderTop": "#035555",
        "tab.hoverBackground": "#375B9D15",
        
        // 列表和树
        "list.activeSelectionBackground": "#375B9D",
        "list.activeSelectionForeground": "#E6DBD6",
        "list.inactiveSelectionBackground": "#375B9D30",
        "list.hoverBackground": "#375B9D15",
        "list.highlightForeground": "#035555"
    },
    "tokenColors": [
        {
            "name": "Keywords",
            "scope": [
                "keyword",
                "storage.type",
                "storage.modifier",
                "keyword.control",
                "keyword.operator.new"
            ],
            "settings": {
                "foreground": "#035555",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Types",
            "scope": [
                "entity.name.type",
                "entity.name.class",
                "entity.name.struct",
                "entity.name.enum",
                "entity.name.interface",
                "support.type",
                "support.class"
            ],
            "settings": {
                "foreground": "#035555",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Functions",
            "scope": [
                "entity.name.function",
                "support.function",
                "meta.function-call"
            ],
            "settings": {
                "foreground": "#375B9D"
            }
        },
        {
            "name": "Variables",
            "scope": [
                "variable",
                "variable.other",
                "variable.parameter",
                "variable.language"
            ],
            "settings": {
                "foreground": "#000202"
            }
        },
        {
            "name": "Constants",
            "scope": [
                "constant",
                "constant.numeric",
                "constant.language",
                "constant.character",
                "constant.other"
            ],
            "settings": {
                "foreground": "#035555",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Strings",
            "scope": [
                "string",
                "string.template",
                "string.quoted"
            ],
            "settings": {
                "foreground": "#375B9D"
            }
        },
        {
            "name": "Comments",
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "foreground": "#353642",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Operators",
            "scope": [
                "keyword.operator",
                "punctuation.definition"
            ],
            "settings": {
                "foreground": "#035555"
            }
        },
        {
            "name": "Decorators",
            "scope": [
                "meta.decorator",
                "variable.annotation"
            ],
            "settings": {
                "foreground": "#375B9D"
            }
        },
        {
            "name": "Support Functions",
            "scope": [
                "support.function",
                "support.macro"
            ],
            "settings": {
                "foreground": "#375B9D"
            }
        },
        {
            "name": "Invalid",
            "scope": [
                "invalid",
                "invalid.illegal"
            ],
            "settings": {
                "foreground": "#FF3333"
            }
        },
        {
            "name": "CSS Properties",
            "scope": [
                "support.type.property-name",
                "support.type.vendored.property-name"
            ],
            "settings": {
                "foreground": "#035555"
            }
        },
        {
            "name": "CSS Values",
            "scope": [
                "support.constant.property-value",
                "support.constant.vendored.property-value"
            ],
            "settings": {
                "foreground": "#375B9D"
            }
        },
        {
            "name": "CSS Units",
            "scope": [
                "keyword.other.unit"
            ],
            "settings": {
                "foreground": "#035555"
            }
        },
        {
            "name": "Markdown Headers",
            "scope": [
                "markup.heading"
            ],
            "settings": {
                "foreground": "#035555",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown Links",
            "scope": [
                "markup.underline.link",
                "string.other.link"
            ],
            "settings": {
                "foreground": "#375B9D"
            }
        },
        {
            "name": "Markdown Bold",
            "scope": [
                "markup.bold"
            ],
            "settings": {
                "foreground": "#035555",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown Italic",
            "scope": [
                "markup.italic"
            ],
            "settings": {
                "foreground": "#000202",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Markdown Quotes",
            "scope": [
                "markup.quote"
            ],
            "settings": {
                "foreground": "#375B9D",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Numbers",
            "scope": [
                "constant.numeric",
                "constant.numeric.integer",
                "constant.numeric.float",
                "constant.numeric.hex",
                "constant.numeric.octal",
                "constant.numeric.binary",
                "constant.numeric.decimal"
            ],
            "settings": {
                "foreground": "#0C7D7D"
            }
        },
        {
            "name": "Language Constants",
            "scope": [
                "constant.language",
                "constant.language.boolean",
                "constant.language.null",
                "constant.language.undefined",
                "constant.language.this",
                "variable.language.this",
                "variable.language.super"
            ],
            "settings": {
                "foreground": "#035555",
                "fontStyle": "italic bold"
            }
        },
        {
            "name": "Punctuation",
            "scope": [
                "punctuation",
                "punctuation.definition.block",
                "punctuation.definition.parameters",
                "punctuation.section",
                "meta.brace"
            ],
            "settings": {
                "foreground": "#375B9D"
            }
        },
        {
            "name": "Special Operators",
            "scope": [
                "keyword.operator.arithmetic",
                "keyword.operator.comparison",
                "keyword.operator.logical",
                "keyword.operator.assignment"
            ],
            "settings": {
                "foreground": "#1A7F7F"
            }
        },
        {
            "name": "Member Access",
            "scope": [
                "punctuation.accessor",
                "meta.property-access"
            ],
            "settings": {
                "foreground": "#375B9D"
            }
        },
        {
            "name": "Template Literals",
            "scope": [
                "string.template",
                "punctuation.definition.template-expression",
                "punctuation.section.embedded"
            ],
            "settings": {
                "foreground": "#2B698F"
            }
        },
        {
            "name": "RegExp",
            "scope": [
                "string.regexp",
                "string.regexp keyword.operator"
            ],
            "settings": {
                "foreground": "#0C7D7D"
            }
        },
        {
            "name": "Object Keys",
            "scope": [
                "meta.object-literal.key",
                "variable.object.property",
                "support.type.property-name"
            ],
            "settings": {
                "foreground": "#375B9D"
            }
        },
        {
            "name": "Class Properties",
            "scope": [
                "variable.other.property",
                "variable.other.object.property"
            ],
            "settings": {
                "foreground": "#375B9D"
            }
        },
        {
            "name": "Function Parameters",
            "scope": [
                "variable.parameter"
            ],
            "settings": {
                "foreground": "#000202",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Import/Export Keywords",
            "scope": [
                "keyword.control.import",
                "keyword.control.export",
                "keyword.control.from"
            ],
            "settings": {
                "foreground": "#035555",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Storage Modifiers",
            "scope": [
                "storage.modifier"
            ],
            "settings": {
                "foreground": "#035555",
                "fontStyle": "bold"
            }
        },
        {
            "name": "JSON Keys",
            "scope": [
                "support.type.property-name.json"
            ],
            "settings": {
                "foreground": "#375B9D"
            }
        },
        {
            "name": "YAML Keys",
            "scope": [
                "entity.name.tag.yaml"
            ],
            "settings": {
                "foreground": "#035555"
            }
        },
        {
            "name": "HTML/XML Tags",
            "scope": [
                "entity.name.tag",
                "punctuation.definition.tag"
            ],
            "settings": {
                "foreground": "#035555"
            }
        },
        {
            "name": "HTML/XML Attributes",
            "scope": [
                "entity.other.attribute-name"
            ],
            "settings": {
                "foreground": "#375B9D",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Git Modified",
            "scope": [
                "markup.changed",
                "meta.diff.header.git",
                "meta.diff.header.from-file",
                "meta.diff.header.to-file"
            ],
            "settings": {
                "foreground": "#375B9D",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Git Added",
            "scope": [
                "markup.inserted"
            ],
            "settings": {
                "foreground": "#035555"
            }
        },
        {
            "name": "Git Deleted",
            "scope": [
                "markup.deleted"
            ],
            "settings": {
                "foreground": "#FF3333"
            }
        }
    ]
} 